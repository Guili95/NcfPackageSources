<Project Sdk="Microsoft.NET.Sdk.Razor">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Version>0.5.15.2-preview1</Version>
    <AssemblyName>Senparc.Xncf.AgentsManager</AssemblyName>
